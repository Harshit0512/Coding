def substrings(arr):
    for i in range(0, len(str)):  
        for j in range(i, len(str)):  
            arr.append(str[i:(j+1)])  

    for i in arr:  
        print(i)
    
if __name__ == '__main__':
    arr = []
    t = int(input())
    for i in range(t):
        str = input()
        substrings(arr)
