def rotations_of_eachother(s1, s2):
    n1 = len(s1) 
    n2 = len(s2) 
    temp = '' 
  
    if n1 != n2: 
        return False
 
    temp = s1 + s1 

    if (temp.count(s2) > 0): 
        return True
    else: 
        return False
    


if __name__ == '__main__':
    s1 = input()
    s2 = input()
    print(rotations_of_eachother(s1, s2))
